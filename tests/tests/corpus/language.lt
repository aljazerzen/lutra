# === std_group_02
func main() -> (
      [
        {1, false, 10},
        {2, false, 21},
        {2, false, 22},
        {1, true, 10}
      ]: [{int64, bool, int64}]
      | std::group(
        func (x: {int64, bool, int64}) -> {x.0, x.2}
      )
      | std::sort(func (x) -> x.key.0 + x.key.1)
      | std::map(func (partition) -> {
        k = partition.key.0,
        v = std::sum(std::map(
          partition.values,
          func (x: {int64, bool, int64}) -> x.2
        )),
      })
    )

# >
[
  {
    k = 1,
    v = 20,
  },
  {
    k = 2,
    v = 21,
  },
  {
    k = 2,
    v = 22,
  },
]

