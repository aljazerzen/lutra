# === complex_00

const x: [int16] = [
  1, 4, 2, 3, 2, 3, 4, 5, 1, 2
]

func main() -> {
  a = 1: int32 + 2,
  {3: int64, 3 + 1: int16},
  {2: int32, 2 + 1: int16},
  hello = (
    x
    | std::map(func (y: int16) -> y + 1)
    | std::filter(func (y: int16) -> !(y > 3))
    # | std::sort(func (x: int16) -> x)
    | std::map(func (y: int16) -> y % 3)
  )
}

# >
{
  a = 3,
  {
    3,
    4,
  },
  {
    2,
    3,
  },
  hello = [
    2,
    0,
    0,
    2,
    0,
  ],
}

